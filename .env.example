GF_ASR_MODEL=small.en
GF_ASR_COMPUTE=float16
GF_ASR_LANGUAGE=en
GF_PAUSE_FLUSH_SEC=0.7
GF_SEGMENT_MAX_SEC=7.0
GF_MT_BEAMS=1
GF_MT_MAX_INPUT=96
GF_MT_MAX_NEW=96
GF_DEVICE=auto

# New live output & recording options
# Maximum lines kept in final TXT (older lines dropped)
GF_MAX_LINES=1000
# Limit words in EN partial before translating ZH (0=disabled)
GF_PARTIAL_WORD_CAP=0
# Save full audio of session: off|wav|flac
GF_SAVE_AUDIO=off
# Path to saved audio file; extension should match GF_SAVE_AUDIO
GF_SAVE_AUDIO_PATH=loquilex/out/session.wav
